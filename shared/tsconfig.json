{
    "compilerOptions": {
        "baseUrl":"../",

        "moduleResolution": "node",
        "target": "ES2020",
        "lib": ["DOM","ESNext"],

        "experimentalDecorators":true,
        
        //"noImplicitAny" : true,
        "noImplicitThis":true,
        "noImplicitReturns": true,
        "noImplicitOverride": true,
        "noFallthroughCasesInSwitch": true,
        "strictBindCallApply":true,
        
    },
    "exclude": [
        "node_modules",
        "**/node_modules/",
    ],    
}
